{
  "manifest_version": 3,
  "name": "TimeLickr",
  "description": "Take control of your time ;)",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "default-icon.png"
  },
  "content_scripts": [{ "matches": ["<all_urls>"], "js": ["Content.js"] }],
  "permissions": ["storage"]
}
